import urllib2

var = (1,2,3,4);

for i,val in enumerate(var):
    user_id=str(var[i])
    response = urllib2.urlopen('https://api.nanigans.com/event.php?s2s=1&type=visit&name=recommendation&user_id=' + user_id + '&sku=' + user_id)
    print response.read
    print 'RESPONSE:', response
    print 'URL:', response.geturl()
    i +=1
